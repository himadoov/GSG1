{
	
    "manifest_version": 3,
    "name": "Change Background",
    "version": "1.0",
    "description": "Click to change background color.",
    "action": {
    "default_popup": "index.html"
    },
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
    "service_worker": "BG.js"
    }
}

